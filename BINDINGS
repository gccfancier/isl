# Build bindings, update and commit them with:
cd build
cmake -DCMAKE_EXPORT_COMPILE_COMMANDS=ON -DISL_INT=gmp  -DCMAKE_INSTALL_PREFIX=${INSTALL_PREFIX} -DCLANG_PREFIX=${CLANG_DIR} -DWITH_EXTRACT_INTERFACE=1 ..
cd ..
cp build/interface/isl.h interface/
cp build/interface/isl-noexceptions.h interface/